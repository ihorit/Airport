<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="airport.view.UserViewController">
   <center>
      <TableView fx:id="tableFlight" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="timeColumn" prefWidth="75.0" text="Time" />
            <TableColumn fx:id="flightColumn" prefWidth="75.0" text="Flight" />
            <TableColumn fx:id="destinationColumn" prefWidth="75.0" text="Destination" />
            <TableColumn fx:id="carriertColumn" prefWidth="75.0" text="Carrier" />
            <TableColumn fx:id="terminalColumn" prefWidth="75.0" text="Terminal" />
          <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
